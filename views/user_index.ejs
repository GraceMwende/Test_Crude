<html lang="en">
   <head>
      <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
   </head>
<body>
   <div href="" class="container">
   <h1><%= title %></h1>
   <a href="/add" class="btn btn-small btn-primary">Add User</a><br><br/>
      <table href="" class="table table-dark table-striped">
         <thead>
            <tr>
               <th>Name</th>
               <th>Email</th>
               <th>Phone No.</th>
               <th>Action</th>
            </tr>
         </thead>
      <tbody>
         <% user.forEach(function(user){ %>
            <tr>
               <td><%= user.name %></td>
               <td><%= user.email %></td>
               <td><%= user.phone_no %></td>
               <td>
                  <a href="edit/ <%= user.id %>" class="btn btn-sm btn-primary">Edit</a>
                  <a href="delete/ <%= user.id %>" class="btn btn-sm btn-danger">Delete</a>
               </td>
            </tr>

        <% }); %>
      </tbody>
      </table>
      </div>
</body>
</html>